<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SecretSetting">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/secretbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="校准" />

    <Button
        android:id="@+id/button38"
        android:layout_width="132dp"
        android:layout_height="120dp"
        android:layout_marginStart="173dp"
        android:background="@drawable/terminal_artist"
        android:onClick="terminalsetting"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secretbar" />

    <Button
        android:id="@+id/button39"
        android:layout_width="132dp"
        android:layout_height="120dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/controldata_artist"
        android:onClick="controldata"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button38"
        app:layout_constraintTop_toBottomOf="@+id/secretbar" />

    <Button
        android:id="@+id/button40"
        android:layout_width="132dp"
        android:layout_height="120dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/outfactory_artist"
        android:onClick="outfactory"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button39"
        app:layout_constraintTop_toBottomOf="@+id/secretbar" />

    <Button
        android:id="@+id/button41"
        android:layout_width="132dp"
        android:layout_height="120dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/installcheck_artist"
        android:onClick="installcheck"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button40"
        app:layout_constraintTop_toBottomOf="@+id/secretbar" />
</androidx.constraintlayout.widget.ConstraintLayout>